{
  "name": "elm-search",
  "version": "1.0.0",
  "description": "an API search engine for all published Elm packages",
  "main": "src/scripts/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "compile-setup": "elm make src/scripts/Setup.elm --yes --output dist/setup.js",
    "compile-main": "elm make dist/Main.elm --yes --output dist/main.js",
    "build-main": "src/scripts/index.js dist/setup.js dist/cache/ dist/Main.elm",
    "make": "npm run compile-setup && npm run build-main && npm run compile-main",
    "minify": "uglifyjs dist/main.js --output dist/main.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klaftertief/elm-search.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/klaftertief/elm-search/issues"
  },
  "homepage": "https://github.com/klaftertief/elm-search#readme",
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-test": "^0.18.9",
    "uglify-js": "^3.0.0",
    "xhr2": "^0.1.4"
  }
}
